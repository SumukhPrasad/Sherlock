<h1>Search</h1>

<%= form_with url: :search, method: :get do |form| %>
     <%= form.label :query, "Search for:" %>
     <%= form.text_field :query %>
     <%= form.submit "Search" %>
     <%= link_to "Reset", search_path %>
<% end %>


<%= render partial: "search/search_entry", collection: @search_entries %>