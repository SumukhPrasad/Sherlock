<% @items.each do |item| %>
     <% for i in 1..item.quantity do %>
          <div class="printdiv">
               <%= item.print_content.html_safe %>
               <p><%= item.name %></p>
          </div>
     <% end %>
<% end %>

<p><i>Print sheet generated by Sherlock <%= Time.now.strftime("on %d/%m/%Y at %H:%M") %>.</i></p>